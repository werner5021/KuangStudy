<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	     xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	     version="4.0" 
	     metadata-complete="true">	
	     
	<servlet>
		<servlet-name>CookieDemo01</servlet-name>
		<servlet-class>com.werner.servlet.CookieDemo01</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CookieDemo01</servlet-name>
		<url-pattern>/cd01</url-pattern>
	</servlet-mapping>
	     
	<servlet>
		<servlet-name>CookieDemo03</servlet-name>
		<servlet-class>com.werner.servlet.CookieDemo03</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CookieDemo03</servlet-name>
		<url-pattern>/cd03</url-pattern>
	</servlet-mapping>
	     
	<servlet>
		<servlet-name>SessionDemo01</servlet-name>
		<servlet-class>com.werner.servlet.SessionDemo01</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SessionDemo01</servlet-name>
		<url-pattern>/sd01</url-pattern>
	</servlet-mapping>
	     
	  <error-page>
	  	<error-code>404</error-code>
	  	<location>/error/404.jsp</location>	  	
	  </error-page>
	  <error-page>
	  	<error-code>500</error-code>
	  	<location>/error/500.jsp</location>
	  </error-page>
	     
	     
	     
</web-app>
